{
  "name": "story-dsl",
  "displayName": "Story DSL",
  "description": "Language support for Story DSL",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [{
      "id": "story",
      "aliases": ["Story DSL", "story"],
      "extensions": [".story"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "story",
      "scopeName": "source.story",
      "path": "./syntaxes/story.tmLanguage.json"
    }],
    "snippets": [{
      "language": "story",
      "path": "./snippets/story.json"
    }]
  },
  "activationEvents": [
    "onLanguage:story"
  ],
  "main": "src/extension.js"
}